@model OWLNEXT.ViewModels.RatePairsModels

<div>
    @foreach(var item in Model.RateMoney)
     {
        <p>La paire de devise @item.Instrument a un taux e change de @item.Rate et le dernier changement de a été fait le @item.Date</p>
     }
    <p>Taux de chage journée</p>
    @foreach (var item in Model.DictionaryRatePairsMinAndMaxDependingOnTheDate)
    {
        <p>La paire de devise @item.Key a eu un taux de change minimum aujourd'hui de @item.Value[0] et a un max de @item.Value[1]</p>
    }
</div>
